{
  "name": "@iziwork/consent-manager",
  "version": "5.3.0",
  "description": "Drop-in consent management plugin for analytics.js",
  "keywords": [
    "gdpr",
    "tracking",
    "analytics",
    "analytics.js"
  ],
  "repository": "git://github.com/Iziwork/consent-manager.git",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/Iziwork"
  },
  "license": "MIT",
  "main": "commonjs/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "commonjs",
    "esm",
    "standalone"
  ],
  "engines": {
    "node": "16.10.0",
    "npm": "please-use-yarn"
  },
  "packageManager": "yarn@3.1.0",
  "scripts": {
    "test": "jest src/__tests__",
    "prepublishOnly": "yarn run clean && yarn run build",
    "postpublish": "yarn build-storybook && yarn deploy-storybook --existing-output-dir=storybook-static",
    "dev": "concurrently 'yarn build-standalone --watch' 'yarn start-storybook -s ./ -p 9009'",
    "build-commonjs": "tsc --outDir commonjs --module CommonJS --inlineSourceMap",
    "build-esm": "tsc --module es2015 --outDir esm --inlineSourceMap",
    "build-standalone": "webpack",
    "build": "concurrently --names 'commonjs,esm,standalone' 'yarn run build-commonjs' 'yarn run build-esm' 'yarn run build-standalone'",
    "clean": "rm -rf commonjs esm standalone storybook-static types",
    "clean:all": "rm -rf node_modules/ .eslintcache && yarn run clean",
    "deploy-storybook": "storybook-to-ghpages",
    "standalone-hash": "shasum -b -a 256 standalone/consent-manager.js | xxd -r -p | base64",
    "build-storybook": "yarn build-standalone && build-storybook && cp -r ./standalone ./storybook-static/standalone",
    "lint": "eslint 'src/**/*.{ts,tsx}'"
  },
  "dependencies": {
    "@segment/in-regions": "~1.2.2",
    "@segment/top-domain": "~3.0.1",
    "emotion": "~9.2.12",
    "isomorphic-fetch": "~3.0.0",
    "js-cookie": "~3.0.1",
    "nanoid": "~3.1.30",
    "react-emotion": "~9.2.12"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@segment/in-eu": "0.3.0",
    "@size-limit/preset-small-lib": "4.12.0",
    "@storybook/addon-actions": "6.1.21",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.1.21",
    "@storybook/addon-options": "5.3.21",
    "@storybook/addon-storysource": "6.1.21",
    "@storybook/addons": "6.1.21",
    "@storybook/react": "6.1.21",
    "@storybook/storybook-deployer": "2.8.10",
    "@types/enzyme": "3.10.10",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/isomorphic-fetch": "0.0.35",
    "@types/jest": "26.0.24",
    "@types/js-cookie": "3.0.1",
    "@types/lodash-es": "4.17.5",
    "@types/nanoid": "2.1.0",
    "@types/node": "16.11.10",
    "@types/react-dom": "16.9.14",
    "@types/segment-analytics": "0.0.34",
    "@types/sinon": "9.0.11",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "babel-loader": "8.2.3",
    "concurrently": "6.4.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "eslint": "7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.27.1",
    "evergreen-ui": "5.1.2",
    "file-loader": "6.2.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "lodash-es": "4.17.21",
    "nock": "13.2.1",
    "preact": "10.6.1",
    "prettier": "2.5.0",
    "pretty-quick": "3.1.2",
    "prismjs": "1.25.0",
    "raw-loader": "4.0.2",
    "react": "16.14.0",
    "react-docgen-typescript-loader": "3.7.2",
    "react-dom": "16.14.0",
    "react-syntax-highlighter": "15.4.5",
    "sinon": "9.2.4",
    "size-limit": "4.12.0",
    "ts-jest": "26.5.6",
    "ts-loader": "8.3.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "webpack": "4.46.0",
    "webpack-cli": "4.9.1"
  },
  "peerDependencies": {
    "lodash-es": "^4",
    "react": "^16 || ^17",
    "react-dom": "^16 || ^17"
  },
  "jest": {
    "preset": "ts-jest/presets/js-with-ts",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!lodash-es)"
    ],
    "setupFiles": [
      "./test/setupTests.ts"
    ]
  },
  "size-limit": [
    {
      "path": "esm/index.js",
      "limit": "60 KB"
    },
    {
      "path": "standalone/consent-manager.js",
      "limit": "60 KB"
    }
  ],
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "storybook-deployer": {
    "gitUsername": "GitHub Pages Bot",
    "gitEmail": "friends@iziwork.com",
    "commitMessage": "Deploy Storybook to GitHub Pages [ci skip]"
  },
  "resolutions": {
    "@types/react": "16.14.21"
  }
}
